
$('.btn-rating<%= @selected_ed_tracker %>').html('<%= link_to (current_user_liked_card?(@edtracker) ? image_tag('smallheart.png', alt: 'Heart', style: "height: 20px; width: 20px; margin-right: 10px;") : image_tag('smallheart_o.png', alt: 'Heart Outline', style: "height: 20px; width: 20px; margin-right: 10px;")), card_liked_destroy_path(edtracker_id: @edtracker), remote: true, method: :delete %> <%= @edtracker.card_likes.count %>')

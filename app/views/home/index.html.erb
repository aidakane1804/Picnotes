      }

      .pic-img::after {
        font-size: 18px;
      }

      .pic-img {
        padding: 40px 15px 15px;
      }

      .write-text {
        padding: 40px 15px 15px;
        margin: 10px 0;
        width: 100%;
      }

      .note {
        padding: 20px;
        width: 100%;
        min-height: 270px;
      }

      .write-text::after {
        font-size: 18px;
      }

      .note h2 {
        font-size: 26px;
        line-height: 10px;
        margin-bottom: 18px;
        margin-top: 20px;
      }

      .note p {
        font-size: 23px;
        line-height: 32px;
        width: 100%;
      }

      .sub-mit {
        padding: 15px;
      }

      .sub-mit-text {
